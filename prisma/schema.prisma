// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

//npx prisma db push

model Connection {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  description    String
  queueName      String
  host           String
  port           Int
  role           String
  numConnections Int
  header         String
  probe          String
  memo           String
}
